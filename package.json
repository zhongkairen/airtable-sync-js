{
  "name": "airtable-sync-js",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:class-order": "eslint --fix src -c .eslintrc.cjs",
    "start": "node --no-warnings --experimental-specifier-resolution=node src/index.js",
    "dev": "node --no-warnings --experimental-specifier-resolution=node src/index.js -d",
    "test": "mocha --no-warnings --reporter dot 'test/**/*.test.js' --ignore 'test/**/*.integration.test.js'",
    "test:integration": "mocha --no-warnings --reporter spec 'test/**/*.integration.test.js'",
    "reinstall": "npm uninstall $(npm ls --depth=0 --parseable | tail -n +2 | sed 's|.*/||') && npm install"
  },
  "author": "Zhongkai Ren",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "mocha": "^10.8.2",
    "prettier": "^3.3.3",
    "sinon": "^19.0.2"
  },
  "dependencies": {
    "airtable-plus": "^1.0.4",
    "date-fns": "^4.1.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-tag": "^2.12.6",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "winston": "^3.15.0",
    "yargs": "^17.7.2"
  }
}
