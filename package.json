  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.2",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "sinon": "^19.0.2"
  },
  "dependencies": {
    "airtable-plus": "^1.0.4",
    "date-fns": "^4.1.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-tag": "^2.12.6",
    "path": "^0.12.7",
    "winston": "^3.15.0",
    "yargs": "^17.7.2"
  }
